<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.gov.am.prodam.commons.javafx.infra.CRUDListController">
   <center>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" style="-fx-border-style: solid;" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </TableView>
   </center>
   <top>
      <GridPane style="-fx-border-style: solid;" vgap="5.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <FlowPane hgap="5.0">
               <children>
                  <TextField fx:id="edtPesquisar" prefWidth="500.0" promptText="digite aqui o texto para pesquisar" />
                  <Button fx:id="btnLimparPesquisa" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </FlowPane>
            <FlowPane hgap="5.0" GridPane.rowIndex="1">
               <children>
                  <Button fx:id="btnPesquisar" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="Pesquisar" />
                  <Button fx:id="btnIncluir" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="Incluir" />
                  <Button fx:id="btnSair" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="Fechar" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </FlowPane>
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </GridPane>
   </top>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</BorderPane>
